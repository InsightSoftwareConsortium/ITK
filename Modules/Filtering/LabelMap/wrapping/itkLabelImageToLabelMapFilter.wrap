itk_wrap_class("itk::ScanlineFilterCommon" POINTER)
  foreach(t ${WRAP_ITK_USIGN_INT})
    foreach(d ${ITK_WRAP_IMAGE_DIMS})
      # image -> label collection image
      itk_wrap_template("${ITKM_I${t}${d}}${ITKM_LM${d}}" "${ITKT_I${t}${d}}, ${ITKT_LM${d}}")
    endforeach()
  endforeach()
itk_end_wrap_class()

