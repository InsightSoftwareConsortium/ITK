itk_wrap_class("itk::PointBasedSpatialObject" POINTER)
  foreach(d ${ITK_WRAP_IMAGE_DIMS})
    itk_wrap_template("Surface${d}" "${d}, itk::SurfaceSpatialObjectPoint<${d}>")
  endforeach()
itk_end_wrap_class()

itk_wrap_class("itk::SurfaceSpatialObject" POINTER)
  foreach(d ${ITK_WRAP_IMAGE_DIMS})
    itk_wrap_template(${d} ${d})
  endforeach()
itk_end_wrap_class()
