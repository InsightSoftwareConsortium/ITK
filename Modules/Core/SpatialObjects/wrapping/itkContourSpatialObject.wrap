itk_wrap_class("itk::PointBasedSpatialObject" POINTER)
  foreach(d ${ITK_WRAP_IMAGE_DIMS})
    itk_wrap_template("Contour${d}" "${d}, itk::ContourSpatialObjectPoint<${d}>")
  endforeach()
itk_end_wrap_class()

itk_wrap_class("itk::ContourSpatialObject" POINTER)
