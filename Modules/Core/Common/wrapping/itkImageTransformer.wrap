itk_wrap_class("itk::ImageTransformer" POINTER)
  itk_wrap_image_filter("${WRAP_ITK_ALL_TYPES}" 1)

  foreach(d ${ITK_WRAP_IMAGE_DIMS})
    foreach(type ${WRAP_ITK_SCALAR})
      itk_wrap_template("${ITKM_VI${type}${d}}"  "${ITKT_VI${type}${d}}")
    endforeach(type)
  endforeach()
itk_end_wrap_class()
